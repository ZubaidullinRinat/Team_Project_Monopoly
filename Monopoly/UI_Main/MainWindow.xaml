    <Window.Resources>
        <Style x:Key="HighlightRectangle" TargetType="{x:Type Rectangle}">
            <Style.Resources>
                <Storyboard x:Key="RectAnim">
                    <ColorAnimation Storyboard.TargetProperty="(Rectangle.Stroke).(SolidColorBrush.Color)" From="Black" To="Gold"/>
                    <DoubleAnimation Storyboard.TargetProperty="(Rectangle.StrokeThickness)" To="5"/>
                </Storyboard>
            </Style.Resources>
            <Style.Triggers>
                <EventTrigger RoutedEvent="UIElement.MouseEnter">
                    <BeginStoryboard x:Name="RectAnim_begin" Storyboard="{StaticResource RectAnim}"/>
                </EventTrigger>
                <EventTrigger RoutedEvent="UIElement.MouseLeave">
                    <RemoveStoryboard BeginStoryboardName="RectAnim_begin"/>
                </EventTrigger>
            </Style.Triggers>
        </Style>
        <SolidColorBrush x:Key="TextBrush">Red</SolidColorBrush>

        <Storyboard x:Key="TextRedAlertAnimation" Duration="0:0:3" >
            <ColorAnimation Storyboard.TargetProperty="(Label.Foreground).(SolidColorBrush.Color)"
                                    Storyboard.TargetName="gotojail_upper"
                                    To="{Binding Source={StaticResource TextBrush}, Path=Color}"
                                    AutoReverse="True"
                                    Duration="0:0:1"/>
        </Storyboard>
    </Window.Resources>
