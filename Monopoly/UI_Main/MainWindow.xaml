        <Style x:Key="HighlightRectangle" TargetType="{x:Type Rectangle}">
            <Style.Resources>
                <Storyboard x:Key="RectAnim">
                    <ColorAnimation SpeedRatio="10" Storyboard.TargetProperty="(Rectangle.Stroke).(SolidColorBrush.Color)" From="Black" To="Gold"/>
                    <DoubleAnimation SpeedRatio="10" Storyboard.TargetProperty="(Rectangle.StrokeThickness)" To="5"/>
                </Storyboard>
            </Style.Resources>
            <Style.Triggers>
                <EventTrigger RoutedEvent="UIElement.MouseEnter">
                    <BeginStoryboard x:Name="RectAnim_begin" Storyboard="{StaticResource RectAnim}"/>
                </EventTrigger>
                <EventTrigger RoutedEvent="UIElement.MouseLeave">
                    <RemoveStoryboard BeginStoryboardName="RectAnim_begin"/>
                </EventTrigger>
            </Style.Triggers>
        </Style>
    </Window.Resources>
