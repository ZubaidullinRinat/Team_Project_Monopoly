                <TextBlock.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform ScaleX="0.8" ScaleY="0.8"/>
                        <SkewTransform/>
                        <RotateTransform/>
                        <TranslateTransform/>
                    </TransformGroup>
                </TextBlock.RenderTransform>
            </TextBlock>
            <TextBlock x:Name="label_3_lower" Text="{Binding Player2_Money}" FontFamily="Calibri" FontWeight="Bold" FontSize="20" TextWrapping="Wrap" TextAlignment="Center" RenderTransformOrigin="0.5,0.5" Height="35" Canvas.Left="310" Canvas.Top="86" Width="133">
                <TextBlock.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform ScaleX="0.8" ScaleY="0.8"/>
                        <SkewTransform/>
                        <RotateTransform/>
                        <TranslateTransform/>
                    </TransformGroup>
                </TextBlock.RenderTransform>
            </TextBlock>
            <TextBlock x:Name="label_4_lowey" Text="{Binding Player3_Money}" FontFamily="Calibri" FontWeight="Bold" FontSize="20" TextWrapping="Wrap" TextAlignment="Center" RenderTransformOrigin="0.5,0.5" Height="35" Canvas.Left="460" Canvas.Top="90" Width="133">
                <TextBlock.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform ScaleX="0.8" ScaleY="0.8"/>
                        <SkewTransform/>
                        <RotateTransform/>
                        <TranslateTransform/>
                    </TransformGroup>
                </TextBlock.RenderTransform>
            </TextBlock>
        </Canvas>
        <Canvas HorizontalAlignment="Left" Height="390" Margin="778,240,0,0" VerticalAlignment="Top" Background="MintCream" Width="612">

            <Button x:Name="button" Content="Следующий ход" HorizontalAlignment="Left" VerticalAlignment="Top" Width="139" Command="{Binding NewMove}" Height="44" Canvas.Left="437" Canvas.Top="328">
                <Button.Triggers>
                    <EventTrigger RoutedEvent="ButtonBase.Click">
                        <BeginStoryboard>
                            <Storyboard>
                                <ObjectAnimationUsingKeyFrames
                                                   Storyboard.TargetProperty="Visibility">
                                    <DiscreteObjectKeyFrame KeyTime="0:0:0"
                                                Value="{x:Static Visibility.Visible}"/>
                                </ObjectAnimationUsingKeyFrames>
                            </Storyboard>
                        </BeginStoryboard>
                    </EventTrigger>
                </Button.Triggers>
            </Button>
            <Button x:Name="button1" Content="Button" HorizontalAlignment="Left" VerticalAlignment="Top" Width="88" Height="47" Canvas.Left="312" Canvas.Top="221"/>
            <Button x:Name="button2" Content="Button" HorizontalAlignment="Left" VerticalAlignment="Top" Width="88" Height="47" Canvas.Left="312" Canvas.Top="273"/>
            <Button x:Name="button3" Content="Button" HorizontalAlignment="Left" VerticalAlignment="Top" Width="88" Height="47" Canvas.Left="312" Canvas.Top="325"/>
            <ListView x:Name="listView" Height="362" Canvas.Left="10" Canvas.Top="10" Width="297" ItemsSource="{Binding CurrentUser}">
                <ListView.View>
                    <GridView>
                        <GridView.Columns>
                            <GridViewColumn Header="Street Name" DisplayMemberBinding="{Binding Name}" ></GridViewColumn>
                        </GridView.Columns>
                    </GridView>
                </ListView.View>
            </ListView>
        </Canvas>

    </Grid>
</Window>
